<?php
class EmpresaCampanhaResponsaveis {
	
	public $idEmpresaCampanhaResponsaveis;
	public $oCampanha;
	public $oEmpresacampanha;
	public $oResponsaveis;
	public $situacao;
	
	function __construct($idEmpresaCampanhaResponsaveis = NULL, Campanha $oCampanha = NULL, Empresacampanha $oEmpresacampanha = NULL, Responsaveis $oResponsaveis = NULL, $situacao = NULL){
		$this->idEmpresaCampanhaResponsaveis = $idEmpresaCampanhaResponsaveis;
		$this->oCampanha = $oCampanha;
		$this->oEmpresacampanha = $oEmpresacampanha;
		$this->oResponsaveis = $oResponsaveis;
		$this->situacao = $situacao;
	}
}