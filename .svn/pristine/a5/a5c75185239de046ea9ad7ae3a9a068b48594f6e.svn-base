h2 {
    text-align: center;
    padding-top: 20px 0;
    font-size: 30px;
    font-weight: 300;
    font-family: "Open Sans",sans-serif;
   }

h3 {
    text-align: center;
    padding-top: 20px 0;
    font-size: 20px;
    font-weight: 300;
    font-family: "Open Sans",sans-serif;
    }
    
    h5 {
    text-align: left;
    padding-top: 10px 0;
    font-size: 12px;
    font-weight: 300;
    font-family: "Open Sans",sans-serif;
    }

.table-bordered {
    border: 1px solid #ddd !important;
    }

table caption {
    padding: .5em 0;
    }

table tfoot tr td {
    text-align: center !important;
    }

@media (max-width: 39.9375em) {
  .tablesaw-stack tbody tr:not(:last-child) {
    border-bottom: 2px solid #0B0B0D;
  }
}

.titulo {
    padding-top: 30px;
    font-size: 14px;
    }

label {
    display: block;
    margin-bottom: 0;
    }

td :first-child {
    font-size: 70%; 
    padding-bottom: 10px; color: #2e2e2e;
    }

td :last-child {
    font-size: 100%; color: #2d2d2e;
    }

.portlet.light.bordered {
    border: 1px solid #bdc3c7 !important;
    }

.portlet.light {
    padding: 12px 20px 2px;
    background-color: #fff;
    }

.logo {
    margin: 10px auto 0;
    padding: 15px;
    text-align: center;
    }

.caption-helper {
    color: #7f8c8d;
    font-size: 10px;
    font-weight: 400;
    padding-top: 20px;
    }

.obs{
    padding-bottom: 30px;
    }