<?php
class Municipio {
	
	public $idMunicipio;
	public $regiao;
	public $uf;
	public $municipio;
	public $microregiao;
	public $tipologia;
	public $status;
	
	function __construct($idMunicipio = NULL, $regiao = NULL, $uf = NULL, $municipio = NULL, $microregiao = NULL, $tipologia = NULL, $status = NULL){
		$this->idMunicipio = $idMunicipio;
		$this->regiao = $regiao;
		$this->uf = $uf;
		$this->municipio = $municipio;
		$this->microregiao = $microregiao;
		$this->tipologia = $tipologia;
		$this->status = $status;
	}
}