<?php
class HistoricoEdicaoEmail {
	
	public $idHistoricoEdicaoEmail;
	public $email_antigo;
	public $email_novo;
	public $usuario;
	public $dt_alteracao;
	public $cnpj;
	
	function __construct($idHistoricoEdicaoEmail = NULL, $email_antigo = NULL, $email_novo = NULL, $usuario = NULL, $dt_alteracao = NULL, $cnpj = NULL){
		$this->idHistoricoEdicaoEmail = $idHistoricoEdicaoEmail;
		$this->email_antigo = $email_antigo;
		$this->email_novo = $email_novo;
		$this->usuario = $usuario;
		$this->dt_alteracao = $dt_alteracao;
		$this->cnpj = $cnpj;
	}
}